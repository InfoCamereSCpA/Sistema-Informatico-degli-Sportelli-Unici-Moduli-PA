            <xsd:element name="non_sottoposto_tutela" type="xsd:boolean"/>
            <xsd:element name="sottoposto_tutela" type="xsd:boolean">

                <xsd:annotation>
                    <xsd:documentation>si allega la documentazione necessaria ai fini del rilascio del parere/nulla
                        osta
                    </xsd:documentation>
