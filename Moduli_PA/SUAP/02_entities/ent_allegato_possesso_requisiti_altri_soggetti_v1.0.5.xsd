                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="dichiarazionePresaVisioneInformativaPrivacy" type="xsd:boolean" minOccurs="0" />
                    <xsd:element name="documentoIdentitaSoggetto" type="efile:file_ctype" minOccurs="0"  />
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>


    <xsd:element name="allegato_possesso_requisiti_altri_soggetti" type="eallposreqaltsog:allegato_possesso_requisiti_altri_soggetti_ctype"/>

