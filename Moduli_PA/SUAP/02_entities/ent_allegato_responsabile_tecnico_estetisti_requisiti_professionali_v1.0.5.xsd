<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"

            xmlns:eallrestecestreqpro="../02_entities/allegato_responsabile_tecnico_estetisti_requisiti_professionali"
            xmlns:eallperana="../02_entities/allegato_persona_anagrafica"

            xmlns:efile="../02_entities/files"

            targetNamespace="../02_entities/allegato_responsabile_tecnico_estetisti_requisiti_professionali"
            elementFormDefault="qualified" attributeFormDefault="qualified"
            version="1.0.5" xml:lang="it">

    <xsd:import namespace="../02_entities/files" schemaLocation="../02_entities/ent_files_v1.0.5.xsd"/>
    <xsd:import namespace="../02_entities/allegato_persona_anagrafica" schemaLocation="../02_entities/ent_allegato_persona_anagrafica_v1.0.5.xsd"/>


    <xsd:complexType name="allegato_responsabile_tecnico_estetisti_requisiti_professionali_ctype">
        <xsd:complexContent>
            <xsd:extension base="eallrestecestreqpro:ResponsabileTecnicoRequisitiProfessionaliBaseType">
                <xsd:sequence>
                    <xsd:element name="dichiarazioni" type="eallrestecestreqpro:DichiarazioniEstetistiRequisitiProfessionaliType"/>
                    <xsd:element name="allegatoDocumentoIdentitaResposabileTecnico" type="efile:file_ctype" />
                    <xsd:element name="allegatoAccettazioneResposabileTecnico" type="efile:file_ctype" />
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="DichiarazioniEstetistiRequisitiProfessionaliType" abstract="true">
        <xsd:choice>
            <xsd:element name="possessoQualificaProfessionale" type="eallperana:AttoRilasciatoDaType"/>
            <xsd:element name="abilitazioneProfessionale">
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="titolare" type="eallperana:ImpresaREAType"/>
                        <xsd:element name="socio" type="eallperana:ImpresaREAType"/>
                        <xsd:element name="direzione" type="eallperana:ImpresaREAType"/>
                        <xsd:element name="dipendente">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="impresaStudioMedico" type="xsd:string"/>
                                    <xsd:element name="periodoDalAl" type="eallperana:PeriodoDateType"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="frequenzaCorso">
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="corsoRegionale" type="eallperana:AttoRilasciatoDaType" />
                        <xsd:element name="corsoScuolaProfessionale" type="eallperana:AttoRilasciatoDaType"/>
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="riconoscimentoQualificaEsteraAutoritaItaliana" type="eallperana:AttoBaseType"/>
            <xsd:element name="altro" type="eallrestecestreqpro:AltroRequisitoProfessionaleBaseType"/>
        </xsd:choice>
    </xsd:complexType>

    <xsd:complexType name="AltroRequisitoProfessionaleBaseType" abstract="true"/>
    <xsd:complexType name="AltroRequisitoProfessionaleType">
        <xsd:complexContent>
            <xsd:extension base="eallrestecestreqpro:AltroRequisitoProfessionaleBaseType">
                <xsd:sequence>
                    <xsd:element name="descrizione" type="xsd:string" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="ResponsabileTecnicoRequisitiProfessionaliBaseType" abstract="true">
        <xsd:sequence>
            <xsd:element name="responsabileTecnico" type="eallperana:allegato_persona_anagrafica_ctype"/>
            <xsd:element name="inQualita">
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="dittaIndividuale" type="xsd:string"/>
                        <xsd:element name="societa" type="xsd:string"/>
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="dichiarazionePresaVisioneInformativaPrivacy" type="xsd:boolean" />
        </xsd:sequence>
    </xsd:complexType>



    <xsd:element name="allegato_responsabile_tecnico_estetisti_requisiti_professionali" type="eallrestecestreqpro:allegato_responsabile_tecnico_estetisti_requisiti_professionali_ctype"/>

</xsd:schema>