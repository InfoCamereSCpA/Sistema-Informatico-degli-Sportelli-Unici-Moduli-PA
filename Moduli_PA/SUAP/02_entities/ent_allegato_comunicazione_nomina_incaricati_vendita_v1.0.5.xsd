            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>



    <xsd:element name="allegato_comunicazione_nomina_incaricati_vendita" type="eallcomnomincven:allegato_comunicazione_nomina_incaricati_vendita_ctype"/>

</xsd:schema>