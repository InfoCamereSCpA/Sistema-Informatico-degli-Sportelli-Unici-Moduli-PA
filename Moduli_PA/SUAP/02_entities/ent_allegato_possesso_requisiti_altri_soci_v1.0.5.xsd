                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>


    <xsd:element name="allegato_possesso_requisiti_altri_soci" type="eallposreqaltsoc:allegato_possesso_requisiti_altri_soci_ctype"/>

</xsd:schema>