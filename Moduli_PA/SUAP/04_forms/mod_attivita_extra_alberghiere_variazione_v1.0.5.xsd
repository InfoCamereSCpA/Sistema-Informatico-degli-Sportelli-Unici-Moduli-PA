            <xsd:element name="riepilogo_documentazione"
                         type="srdocextraalb:riepilogo_documentazione_ctype" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="comunicazione_variazione_extra_alberghiera"
