            xmlns:sschedaanag="../03_sections/scheda_anagrafica"
            xmlns:srdocextraalb="../03_sections/riepilogo_documentazione_extra_alberghiere"
            targetNamespace="../04_forms/attivita_extra_alberghiere_variazione"
            elementFormDefault="qualified" attributeFormDefault="qualified" version="1.0.5" xml:lang="it">

    <xsd:import namespace="../03_sections/scheda_anagrafica"
                schemaLocation="../03_sections/sec_scheda_anagrafica_v1.0.5.xsd"/>
