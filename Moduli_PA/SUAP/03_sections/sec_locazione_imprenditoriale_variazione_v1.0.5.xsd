<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:slocimpvar="../03_sections/locazione_imprenditoriale_variazione"
    xmlns:eanagattloc="../02_entities/anagrafica_attivita_locazioni"
