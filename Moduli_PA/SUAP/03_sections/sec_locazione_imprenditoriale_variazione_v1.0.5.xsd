    <xsd:import namespace="../02_entities/anagrafica_attivita_locazioni" schemaLocation="../02_entities/ent_anagrafica_attivita_locazioni_v1.0.5.xsd"/>
    <xsd:import namespace="../02_entities/estremi_comunicazione" schemaLocation="../02_entities/ent_estremi_comunicazione_v1.0.5.xsd"/>
