            xmlns:eimpeselavpdc="../02_entities/impresa_esecutrice_lavori_pdc"
            xmlns:eoblsalsicaltpdc="../02_entities/obblighi_salute_sicurezza_altpdc"

            targetNamespace="../03_sections/dichiarazioni_pdc" elementFormDefault="qualified"
            attributeFormDefault="qualified" version="1.0.6" xml:lang="it">

    <xsd:import namespace="../02_entities/qualificazione_intervento_pdc" schemaLocation="../02_entities/ent_qualificazione_intervento_pdc_v1.0.6.xsd"/>
    <xsd:import namespace="../02_entities/sanatoria_in_corso_pdc" schemaLocation="../02_entities/ent_sanatoria_in_corso_pdc_v1.0.6.xsd"/>
    <xsd:import namespace="../02_entities/titolo_possesso" schemaLocation="../02_entities/ent_titolo_possesso_v1.0.6.xsd"/>
    <xsd:import namespace="../02_entities/localizzazione_intervento" schemaLocation="../02_entities/ent_localizzazione_intervento_v1.0.6.xsd"/>
    <xsd:import namespace="../02_entities/opere_parti_comuni" schemaLocation="../02_entities/ent_opere_parti_comuni_v1.0.6.xsd"/>
    <xsd:import namespace="../02_entities/regolarita_urbanistica_pdc" schemaLocation="../02_entities/ent_regolarita_urbanistica_pdc_v1.0.6.xsd"/>
    <xsd:import namespace="../02_entities/calcolo_contributo_costruzione_pdc" schemaLocation="../02_entities/ent_calcolo_contributo_costruzione_pdc_v1.0.6.xsd"/>
