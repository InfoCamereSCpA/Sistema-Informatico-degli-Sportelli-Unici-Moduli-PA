    <xsd:complexType name="sezione_dichiarazioni_pdc_ctype">
        <xsd:sequence>
            <xsd:element name="qualificazione_intervento" type="equaintpdc:qualificazione_intervento_base_type" minOccurs="0"/>
            <xsd:element name="sanatoria_in_corso" type="esancorpdc:sanatoria_in_corso_base_type" minOccurs="0"/>
            <xsd:element name="titolo_possesso" type="etitpos:titolo_possesso_ctype" minOccurs="0"/>
            <xsd:element name="localizzazione_intervento" type="elocint:localizzazione_intervento_ctype" minOccurs="0"/>
            <xsd:element name="opere_previste" type="eopeparcom:opere_previste_ctype" minOccurs="0"/>
            <xsd:element name="regolarita_urbanistica" type="eregurbpdc:regolarita_urbanistica_pdc_base_type" minOccurs="0"/>
            <xsd:element name="calcolo_contributo_costruzione" type="ecalconcospdc:calcolo_contributo_base_type" minOccurs="0"/>
            <xsd:element name="tecnici_incaricati_pdc" type="etecincpdc:tecnici_incaricati_dei_lavori_pdc_ctype" minOccurs="0"/>
            <xsd:element name="imprese_esecutrici" type="eimpeselavpdc:imprese_esecutrici_dei_lavori_pdc_ctype" minOccurs="0"/>
            <xsd:element name="obblighi_salute_sicurezza_pdc" type="eoblsalsicaltpdc:obblighi_salute_sicurezza_altpdc_ctype" minOccurs="0"/>

            <xsd:element name="note" type="xsd:string" minOccurs="0"/>
            <xsd:element name="dichiarazione_presa_visione_informativa_privacy" type="xsd:boolean" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="sezione_dichiarazioni_pdc" type="sdicpdc:sezione_dichiarazioni_pdc_ctype"/>

