    <!-- new -->
    <xs:complexType name="QualificazioneInterventoType">
        <xs:complexContent>
            <xs:extension base="suecommon:QualificazioneInterventoBaseType">
                <xs:sequence>
                    <xs:choice>
                        <xs:element name="manutenzioneStraordinaria" type="scia:QualificazioneInterventoRistrutturazioneMutamentoType"/>
                        <xs:element name="manutenzioneRestauro" type="scia:QualificazioneInterventoRistrutturazioneMutamentoType"/>
                        <xs:element name="manutenzioneRistrutturazione" type="scia:QualificazioneInterventoRistrutturazioneMutamentoType"/>

                        <xs:element name="varianteInCorsoDOperaPermessoDiCostruire" type="aggregate:AttoBaseType"/>
                        <xs:element name="varianteInCorsoDOperaSCIA" type="aggregate:AttoBaseType"/>
                        <xs:element name="varianteInCorsoDOperaVariazioneEssenziale" type="basic:EmptyType"/>

                        <xs:element name="mutamentoDestinazioneUsoSingolaUnita">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:choice>
                                        <xs:element name="senzaOpereCategoriaSingolaUnita" type="xs:boolean" />
                                        <xs:element name="senzaOpereTraCategorieSingolaUnita" type="xs:boolean" />
                                        <xs:element name="conOpereCategoriaSingolaUnita" type="xs:boolean" />
                                        <xs:element name="conOpereTraCategorieSingolaUnita" type="xs:boolean" />
                                    </xs:choice>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="mutamentoDestinazioneUsoInteroImmobile">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:choice>
                                        <xs:element name="conOpere" type="xs:boolean" />
