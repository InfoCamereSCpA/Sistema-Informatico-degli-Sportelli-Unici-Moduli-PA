    <xs:complexType name="SanatoriaInCorsoType">
        <xs:complexContent>
            <xs:extension base="suecommon:SanatoriaInCorsoBaseType">
                <xs:sequence>

                    <xs:element name="pagamentoSanzione">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:choice>
                                    <xs:element name="estremiPagamentoSanzione">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="estremiPagamento" type="xs:string" />
                                                <xs:element name="estremiPagamentoData" type="xs:date" />
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="ricevutaPagamento" type="xs:date" />
                                </xs:choice>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                    <xs:element name="sanatoriaSoggetto">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="interventoData" type="xs:date" />
                                <xs:choice>
                                    <xs:element name="estremiPagamentoSanatoria">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="importoSanatoria" type="xs:string" />
                                                <xs:element name="pagamentoData" type="xs:date" />
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="importoRicevutaPagamento" type="xs:string" />
                                </xs:choice>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                    <xs:element name="sanatoriaSoggettoFinoDiecimila">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="interventoData" type="xs:date" />
                                <xs:choice>
                                    <xs:element name="estremiPagamentoSanatoria">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:element name="importoSanatoria" type="xs:string" />
                                                <xs:element name="pagamentoData" type="xs:date" />
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                    <xs:element name="importoRicevutaPagamento" type="xs:string" />
                                </xs:choice>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                    <xs:element name="interventoVarianteInCorsoOpera">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="titoloEdilizio" type="aggregate:AttoBaseType" />
                                <xs:element name="altraDocumentazione" type="xs:boolean" />
                                <xs:element name="estremiVersamento">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element name="importo" type="xs:string" />
                                            <xs:element name="versamentoData" type="xs:date" />
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element name="importoRicevutaVersamento" type="xs:string" />
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
