<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
    xmlns:permcostr="http://www.agid.gov.it/MODULI_PA/SUE/modulo_permesso_costruire"
    xmlns:suecommon="http://www.agid.gov.it/MODULI_PA/SUE/sue_common_components"
    xmlns:moduli="http://www.agid.gov.it/MODULI_PA/moduli_common_components"
    xmlns:aggregate="http://www.agid.gov.it/AgID_aggregate_components"
    xmlns:basic="http://www.agid.gov.it/AgID_basic_components"
    xmlns:enumeration="http://www.agid.gov.it/AgID_enumeration"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    
    targetNamespace="http://www.agid.gov.it/MODULI_PA/SUE/modulo_permesso_costruire"

    elementFormDefault="qualified" attributeFormDefault="qualified" version="1.3.0">

    
    
    
    <xs:import namespace="http://www.agid.gov.it/MODULI_PA/SUE/sue_common_components" schemaLocation="SUE_common_components.xsd"/>    
    <xs:import namespace="http://www.agid.gov.it/MODULI_PA/moduli_common_components" schemaLocation="../MODULI_common_components.xsd"/>
    <xs:import namespace="http://www.agid.gov.it/AgID_basic_components" schemaLocation="../../AgID_Vocabularies/AgID_basic_components.xsd"/>
    <xs:import namespace="http://www.agid.gov.it/AgID_aggregate_components" schemaLocation="../../AgID_Vocabularies/AgID_aggregate_components.xsd"/>
    <xs:import namespace="http://www.agid.gov.it/AgID_enumeration" schemaLocation="../../AgID_Vocabularies/AgID_enumeration.xsd"/>
    
    <!-- ROOT ELEMENT -->
    <xs:element name="ModuloPermessoCostruire" type="permcostr:ModuloPermessoCostruireType" />
    
    <xs:complexType name="ModuloPermessoCostruireType">
        <xs:annotation>
            <xs:documentation>
                RICHIESTA DI PERMESSO DI COSTRUIRE
                (art. 20, d.P.R. 6 giugno 2001, n. 380 – artt. 7, d.P.R. 7 settembre 2010, n. 160)
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            
            <!-- SOGGETTI -->
            <xs:element name="schedaTitolare" type="aggregate:PersonaAnagraficaType">
                <xs:annotation>
                    <xs:documentation>
                        schedaTitolare: in caso di più titolari, la sezione è ripetibile nell’allegato “SOGGETTI COINVOLTI”
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="schedaDitta" type="suecommon:SchedaDittaType" minOccurs="0"/>
            <xs:element name="schedaDelegato" type="aggregate:PersonaAnagraficaType" minOccurs="0"/>
            
            <!-- DICHIARAZIONI -->
            <xs:element name="qualificazioneIntervento" type="suecommon:QualificazioneInterventoBaseType"/>
            <xs:element name="sanatoriaInCorso" type="suecommon:SanatoriaInCorsoBaseType" minOccurs="0" />
            <xs:element name="titoloPossesso" type="suecommon:TitoloPossessoType"/>
            <xs:element name="localizzazioneIntervento" type="suecommon:LocalizzazioneInterventoType"/>
            <xs:element name="operePreviste" type="suecommon:OperePrevisteType"/>
            <xs:element name="newRegolaritaUrbanistica" type="suecommon:NewRegolaritaUrbanisticaBaseType" minOccurs="0"/>
            <xs:element name="calcoloContributoCostruzione" type="suecommon:CalcoloContributoBaseType"/>
            <xs:element name="tecniciIncaricati" type="permcostr:TecniciIncaricatiDeiLavoriType"/>
            <xs:element name="impreseEsecutrici" type="permcostr:ImpreseEsecutriciDeiLavoriType"/>           
            <xs:element name="obblighiSaluteSicurezza" type="permcostr:ObblighiSaluteSicurezzaType"/>
            <xs:element name="dichiarazionePresaVisioneInformativaPrivacy" type="xs:boolean" />
            
            <!-- RELAZIONE TECNICA DI ASSEVERAZIONE -->
            <xs:element name="relazioneTecnicaAsseverazione" type="suecommon:RelazioneTecnicaAsseverazioneType" minOccurs="0" />
            
            <!-- ALLEGATI  -->
            <xs:element name="tabellaAllegati" type="permcostr:TabellaAllegatiType" minOccurs="0"/>

        </xs:sequence>
    </xs:complexType>


    <!-- BASE TYPE -->
    <xs:complexType name="QualificazioneInterventoType">
        <xs:complexContent>
            <xs:extension base="suecommon:QualificazioneInterventoBaseType">
                <xs:sequence>
                    <xs:choice>

                        <xs:element name="newArticolo10" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:choice>
                                        <xs:element name="noMutamento" type="xs:boolean" />
                                        <xs:element name="mutamentoCategoriaSingolaUnita" type="xs:boolean" />
                                        <xs:element name="mutamentoTraCategorieSingolaUnita" type="xs:boolean" />
                                        <xs:element name="mutamentoCategoriaInteroImmobile" type="xs:boolean" />
                                    </xs:choice>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>

                        <xs:element name="assoggettatoSCIA" type="xs:string"/>
                        <xs:element name="conformeDisciplinaUrbanistica" type="basic:EmptyType"/>
                        <xs:element name="derogaPianificazioneUrbanistica">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="densitaEdilizia" minOccurs="0" type="xs:string"/>
                                    <xs:element name="altezza" minOccurs="0" type="xs:string"/>
                                    <xs:element name="distanzaFabbricati" minOccurs="0" type="xs:string"/>
                                    <xs:element name="destinazioneDUso" minOccurs="0" type="xs:string"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="variazioneSostanziale">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:choice>
                                        <xs:element name="titoloUnico" type="aggregate:AttoBaseType"/>
                                        <xs:element name="permessoCostruire" type="aggregate:AttoBaseType"/>
                                        <xs:element name="segnalazioneCertificata" type="aggregate:AttoBaseType"/>
                                     </xs:choice>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                    <xs:element name="procedimentoOrdinario" type="basic:EmptyType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="SanatoriaInCorsoType">
        <xs:complexContent>
            <xs:extension base="suecommon:SanatoriaInCorsoBaseType">
                <xs:sequence>

                        <!-- X.1 -->
                        <xs:element name="sanatoriaSoggetto">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="interventoData" type="xs:date" />
                                    <xs:element name="rilascioPermessoCostruire" type="xs:boolean"/>
<!--                                        <xs:complexType>-->
<!--                                            <xs:sequence>-->
<!--                                                <xs:element name="rilascioPermessoCostruireCheck" type="xs:boolean" />-->
<!--                                            </xs:sequence>-->
<!--                                        </xs:complexType>-->
<!--                                    </xs:element>-->
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>

                        <!-- X.2 -->
                        <xs:element name="interventoPermessoSanatoria">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="interventoData" type="xs:date" />
                                    <!-- X.2.1 -->
                                    <xs:element name="conformitaPresentazioneRealizzazione">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <!-- X.2.1.x -->
                                                <xs:choice>
                                                    <xs:element name="oblazioneDoppioContributoParziale"
                                                                type="xs:boolean"/>
                                                    <xs:element name="condizioneSicurezzaRimozione" type="xs:boolean"/>
                                                </xs:choice>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                    <!-- X.2.2 -->
                                    <xs:element name="conformitaUrbanisticaEdiliziaPresentazioneRealizzazione">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <!-- X.2.2.x -->
                                                <xs:choice>
                                                    <xs:element name="oblazioneDoppioContributoTotale"
                                                                type="xs:boolean"/>
                                                    <xs:element name="condizioneSicurezzaRimozioneDoppia"
                                                                type="xs:boolean"/>
                                                </xs:choice>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>


                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>



    <xs:complexType name="newRegolaritaUrbanisticaType">
        <xs:complexContent>
            <xs:extension base="suecommon:NewRegolaritaUrbanisticaBaseType">
                <xs:sequence>
                    <xs:choice>
                        <xs:element name="nuovaCostruzioneAreaLiberaCheck" type="xs:boolean"/>
                        <xs:group ref="suecommon:NewRegolaritaUrbanisticaPdCOptionGroup"/>
                    </xs:choice>
                    <xs:choice>
                        <xs:element name="pienamenteConforme" type="xs:boolean" />
                        <xs:element name="conformeDocumentazione" type="xs:boolean" />
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>



    <xs:complexType name="CalcoloContributoType">
        <xs:complexContent>
            <xs:extension base="suecommon:CalcoloContributoBaseType">
            <xs:choice>
                <xs:element name="titoloGratuito" type="xs:string"/>
                <xs:element name="titoloOneroso">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:choice>
                                <xs:element name="calcoloSportello" type="basic:EmptyType"/>
                                <xs:element name="calcoloPreventivo" type="basic:EmptyType"/>                                                    
                            </xs:choice>
                            <xs:element name="versamento" minOccurs="0">
                                <xs:complexType>
                                    <xs:choice>
                                        <xs:element name="momentoRitiro" type="basic:EmptyType"/>
                                        <xs:element name="chiedeRateizzazione">   
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="corrispondeInCorso" minOccurs="0" type="basic:EmptyType"/>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:choice>                              
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="oneriUrbanizzazione" minOccurs="0" type="basic:EmptyType"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:choice>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType> 
    

    <xs:complexType name="ObblighiSaluteSicurezzaType">
        <xs:choice>
            <xs:element name="nonRicade" type="basic:EmptyType"/>
            <xs:element name="ricade">
                <xs:complexType>
                    <xs:sequence>
                        <xs:group ref="suecommon:ObblighiSaluteSicurezzaEntitaCantiereGroup"/>
                        <xs:choice>
                            <xs:element name="noNotifica" type="basic:EmptyType"/>
                            <xs:element name="notifica">
                                <xs:complexType>
                                    <xs:choice>
                                        <xs:element name="allega" type="basic:AbstractBaseType"/> 
                                        <xs:element name="invioPrimaInizioLavori" type="basic:EmptyType"/> 
                                    </xs:choice>
                                </xs:complexType>
                            </xs:element>
                        </xs:choice>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="ricadeRiservaPresentaPrimaInizioLavori" type="basic:AbstractBaseType"/>
        </xs:choice>        
    </xs:complexType>


    <xs:complexType name="TipoInterventiOpere">
        <xs:complexContent>
            <xs:extension base="suecommon:TipoInterventiOpereBaseType">
                <xs:sequence>
                    <xs:element name="nuovaCostruzione" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="manufattiEdilizi" type="basic:EmptyType" minOccurs="0"/>
                                <xs:element name="urbanizzazione" type="basic:EmptyType" minOccurs="0"/>
                                <xs:element name="impianti" type="basic:EmptyType" minOccurs="0"/>
                                <xs:element name="tralicci" type="basic:EmptyType" minOccurs="0"/>
                                <xs:element name="manufattiLeggeri" type="basic:EmptyType" minOccurs="0"/>
                                <xs:element name="nuovaCostruzionePregioAmbientale" type="basic:EmptyType" minOccurs="0"/>
                                <xs:element name="depositi" type="basic:EmptyType" minOccurs="0"/>
                                <xs:element name="trasformazioneUrbanistica" type="xs:string" minOccurs="0"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="ristrutturazioneUrbanistica" minOccurs="0" type="basic:EmptyType"/>
                    <xs:element name="ristrutturazioneEdilizia" minOccurs="0" type="basic:EmptyType"/>
                    <xs:element name="assoggettatiSCIA" minOccurs="0" type="xs:string"/>
                    <xs:element name="variantiEssenziali" minOccurs="0" type="basic:EmptyType"/>
                    <xs:element name="variantiCentroStorico" minOccurs="0" type="basic:EmptyType"/>

                    <!-- v2 -->
                    <xs:element name="opereInProgetto" minOccurs="0">
                        <xs:complexType>
                            <xs:choice>
                                <xs:sequence>
                                    <!-- v2.1 -->
                                    <xs:element name="noMutamentoDestinazioneUso" minOccurs="0" type="xs:boolean"/>
                                    <!-- v2.2 -->
                                    <xs:element name="siMutamentoDestinazioneUsoStessaCategoria" minOccurs="0" type="xs:boolean"/>
                                    <!-- v2.3 -->
                                    <xs:element name="siMutamentoDestinazioneUsoTraCategorie" minOccurs="0" type="xs:boolean"/>
                                    <!-- v2.4 -->
                                    <xs:element name="siMutamentoDestinazioneUsoInteroImmobileStessaCategoria" minOccurs="0" type="xs:boolean"/>
                                </xs:sequence>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>

                    <!-- v3 -->
                    <xs:element name="permessoDiCostruireInSanatoria" minOccurs="0" type="xs:boolean"/>

                    <xs:element name="dettagliOpere" minOccurs="0" type="xs:string"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>



