    <xs:complexType name="SanatoriaAsseverazioneType">
        <xs:complexContent>
            <xs:extension base="suecommon:SanatoriaAsseverazioneBaseType">
                <xs:sequence>

                    <!-- X.1 -->
                    <xs:element name="interventoInTotaleDifformita" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="interventoData" type="xs:date"/>
                                <!-- X.1.1 -->
                                <xs:element name="allegaDocumentazione">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element name="allegaDocumentazioneCheck" type="xs:boolean"/>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>

                    <!-- X.2 -->
                    <xs:element name="interventoInParzialeDifformita" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="interventoData" type="xs:date"/>
                                <xs:choice>
                                    <!-- X.2.1 -->
                                    <xs:element name="disciplinaUrbanistica" minOccurs="0">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:choice>
                                                    <!-- X.2.1.1 -->
                                                    <xs:element name="allegaDocumentazioneNecessaria"
                                                                type="xs:boolean"/>
                                                    <!-- X.2.1.2 -->
                                                    <xs:element name="realizzazioneInterventoAtto" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="realizzazioneInterventoAttoData"
                                                                            type="xs:string"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <!-- X.2.1.3 -->
                                                    <xs:element name="realizzazioneInterventoAttesta" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="realizzazioneInterventoAttestaData"
                                                                            type="xs:string" minOccurs="0"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <!-- X.2.1.4 -->
                                                    <xs:element name="permessoCostruireInSanatoria" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <!-- X.2.1.4.x -->
                                                                <xs:element name="noInterventi" type="xs:boolean"
                                                                            minOccurs="0"/>
                                                                <xs:element name="siInterventi" type="xs:boolean"
                                                                            minOccurs="0"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:choice>
                                                <!-- X.2.1.5 -->
                                                <xs:element name="rilevanzaSismica">
                                                    <xs:complexType>
                                                        <xs:choice>
                                                            <!-- X.2.1.5.1 -->
                                                            <xs:element name="bassaSismicita" type="xs:boolean"/>

                                                            <!-- X.2.1.5.2 -->
                                                            <xs:element name="mediaAltaSismicita">
                                                                <xs:complexType>
                                                                    <xs:sequence>
                                                                        <!-- X.2.1.5.2.x -->
                                                                        <xs:choice>
                                                                            <xs:element name="noRilevanzaStrutturale"
                                                                                        type="xs:boolean"/>
                                                                            <xs:element name="haRilevanzaStrutturale"
                                                                                        type="xs:boolean"/>
                                                                        </xs:choice>
                                                                        <!-- X.2.1.5.2.2.1 -->
                                                                        <xs:element name="pubblicaIncolumita" minOccurs="0">
                                                                            <xs:complexType>
                                                                                <xs:sequence>
                                                                                    <!-- X.2.1.5.2.2.1.x -->
                                                                                    <xs:element
                                                                                            name="autorizzazioneSismica"
                                                                                            type="aggregate:AttoBaseType"/>
                                                                                    <xs:element
                                                                                            name="istanzaAutorizzazione"
                                                                                            type="aggregate:AttoBaseType"/>
                                                                                </xs:sequence>
                                                                            </xs:complexType>
                                                                        </xs:element>

                                                                        <!-- X.2.1.5.2.2.2 -->
                                                                        <xs:element name="minorePubblicaIncolumita" minOccurs="0">
                                                                            <xs:complexType>
                                                                                <xs:sequence>
                                                                                    <!-- X.2.1.5.2.2.2.1 -->
                                                                                    <xs:element name="decorsoTermine"
                                                                                                type="xs:boolean"/>
                                                                                    </xs:sequence>
                                                                                </xs:complexType>
                                                                            </xs:element>

                                                                        <!-- X.2.1.5.2.2.3 -->
                                                                        <xs:element name="assenzaPubblicaIncolumita" minOccurs="0">
                                                                            <xs:complexType>
                                                                                <xs:sequence>
                                                                                    <!-- X.2.1.5.2.2.3.1 -->
                                                                                    <xs:element
                                                                                            name="decorsoTermineAssenza"
                                                                                            type="xs:boolean"/>
                                                                                </xs:sequence>
                                                                            </xs:complexType>
                                                                        </xs:element>

                                                                    </xs:sequence>
                                                                </xs:complexType>
                                                            </xs:element>

                                                        </xs:choice>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:complexType>
                                    </xs:element>


                                    <!-- X.2.2 -->
                                    <xs:element name="disciplinaUrbanisticaEdilizia" minOccurs="0">
                                        <xs:complexType>
                                            <xs:sequence>
                                                <xs:choice>
                                                    <!-- X.2.2.1 -->
                                                    <xs:element name="allegaDocumentazioneNecessaria"
                                                                type="xs:boolean"/>
                                                    <!-- X.2.2.2 -->
                                                    <xs:element name="realizzazioneInterventoAtto">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="realizzazioneInterventoAttoData"
                                                                            type="xs:string"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>

                                                    <!-- X.2.2.3 -->
                                                    <xs:element name="realizzazioneInterventoAttesta" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="realizzazioneInterventoAttestaData"
                                                                            type="xs:string" minOccurs="0"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <!-- X.2.2.4 -->
                                                    <xs:element name="permessoCostruireInSanatoria" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <!-- X.2.2.4.x -->
                                                                <xs:element name="noInterventi" type="xs:boolean"
                                                                            minOccurs="0"/>
                                                                <xs:element name="siInterventi" type="xs:boolean"
                                                                            minOccurs="0"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:choice>
                                                <!-- X.2.2.5 -->
                                                <xs:element name="rilevanzaSismica">
                                                    <xs:complexType>
                                                        <xs:choice>


                                                            <!-- X.2.2.5.1 -->
                                                            <xs:element name="bassaSismicita" type="xs:boolean"/>

                                                            <!-- X.2.2.5.2 -->
                                                            <xs:element name="mediaAltaSismicita">
                                                                <xs:complexType>
                                                                    <xs:sequence>
                                                                        <!-- X.2.2.5.2.x -->
